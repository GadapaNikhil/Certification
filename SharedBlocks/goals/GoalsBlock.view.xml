<mvc:View 
controllerName="sap.uxap.sample.ObjectPageOnJSON.Controller.Controller"
xmlns:mvc="sap.ui.core.mvc" xmlns:forms="sap.ui.layout.form"  xmlns="sap.ui.table" xmlns:m="sap.m">
	<Table
				id="table1"
				ariaLabelledBy="title"
				selectionMode="MultiToggle"
				rows="{/modelData}"
				visibleRowCount="7"
				fixedColumnCount="2"
				enableColumnFreeze="true"
				showColumnVisibilityMenu="true">
				<extension>
					<m:OverflowToolbar style="Clear">
						<m:Title id="title" text="Labour"/>
					</m:OverflowToolbar>
				</extension>
				<columns>
					<Column
						width="11rem"
						sortProperty="supplier"
						filterProperty="supplier">
						<m:Label text="Job Catagories " textAlign="Center" width="100%"/>
						<template>
							<m:Text text="{supplier}"/>
						</template>
					</Column>
					<Column
						width="11rem"
						sortProperty="supplier"
						filterProperty="supplier">
						<m:Label text="Description" textAlign="Center" width="100%"/>
						<template>
							<m:Text text="{description}"/>
						</template>
					</Column>
					
					<Column
						width="11rem"
						sortProperty="street"
						filterProperty="street"
						headerSpan="4">
						<multiLabels>
							<m:Label text="31Dec18" textAlign="Center" width="100%"/>
							<m:Label text="Saudi HeadCount" textAlign="Center" width="100%"/>
							<!--<m:Label text="Street" textAlign="Center" width="100%"/>-->
						</multiLabels>
						<template>
							<m:Input value="{street}" wrapping="false"/>
						</template>
					</Column>
					<Column
						width="11rem"
						sortProperty="city">
						<multiLabels>
							<m:Label text="31Dec18"/>
							<m:Label text="Expats Headcount"/>
							<!--<m:Label text="City" textAlign="Center" width="100%"/>-->
						</multiLabels>
						<template>
							<m:Input value="{city}"/>
						</template>
					</Column>
					<Column
						width="11rem"
						sortProperty="phone">
						<multiLabels>
							<m:Label text="31Dec18"/>
							<m:Label text="Total" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Text text="{phone}" />
						</template>
					</Column>
					<Column width="8rem" hAlign="End">
						<multiLabels>
						<m:Label text="31Dec18"/>
							<m:Label text="Saudization%" textAlign="Center" width="100%"/>
						</multiLabels>
						<template>
							<m:Text text="{openOrders}"/>
